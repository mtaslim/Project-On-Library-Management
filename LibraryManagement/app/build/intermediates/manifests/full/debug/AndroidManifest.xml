<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.easydeveloper.librarymanagement"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.easydeveloper.librarymanagement.Activity.Home" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.easydeveloper.librarymanagement.Activity.BookList" />
        <activity android:name="com.example.easydeveloper.librarymanagement.Activity.BookAddUpdate" />
        <activity android:name="com.example.easydeveloper.librarymanagement.Activity.Login" />
        <activity android:name="com.example.easydeveloper.librarymanagement.Activity.BookDetails" />
    </application>

</manifest>